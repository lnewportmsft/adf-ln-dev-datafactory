{
	"name": "FXFDelimitedTextBlob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lgnstorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			},
			"filetype": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@{dataset().filename}.@{dataset().filetype}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}